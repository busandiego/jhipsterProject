<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity BusinessPlace.
    -->
    <changeSet id="20210319015126-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="business_id"
                                 baseTableName="business_place"
                                 constraintName="fk_business_place_business_id"
                                 referencedColumnNames="id"
                                 referencedTableName="business"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="business_place"
                                 constraintName="fk_business_place_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

    </changeSet>
</databaseChangeLog>
